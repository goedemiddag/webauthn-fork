{
    "name": "goedemiddag/webauthn-fork",
    "description": "Authenticate users with just their device, fingerprint or biometric data. Goodbye passwords!",
    "keywords": [
        "darkghosthunter",
        "webauthn",
        "laravel"
    ],
    "homepage": "https://github.com/darkghosthunter/larapass",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Italo Israel Baeza Cabrera",
            "email": "darkghosthunter@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "nyholm/psr7": "^1.3",
        "ramsey/uuid": "^4.0",
        "spomky-labs/base64url": "^2",
        "symfony/psr-http-message-bridge": "^2.0",
        "thecodingmachine/safe": "^1.3.3",
        "web-auth/webauthn-lib": "^4.8"
    },
    "require-dev": {
        "laravel/framework": "^10.0 || ^11.0",,
        "laravel/tinker": "^2.8",
        "phpunit/phpunit": "^9.5.2"
    },
    "autoload": {
        "psr-4": {
            "DarkGhostHunter\\Larapass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DarkGhostHunter\\Larapass\\LarapassServiceProvider"
            ]
        }
    }
}
